export component Legendas inherits HorizontalLayout {
    padding: 25px;
    alignment: center;
    in property <color> color;
    in property <string> legenda;
    Rectangle {
        width: 20px;
        height: 20px;
        border-radius: 5px;
        background: color;
    }

    Text {
        vertical-alignment: center;
        text: legenda;
    }
}
